<div class="modal fade" id="exportModal" tabindex="-1" role="dialog" aria-labelledby="exportModal" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Dispatch Parcel</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form method="POST" id="dataget" action="/ops/checkoutParcel">
            <div class="form-group">
            <span>WayBill:</span>
                <select name="waybill" class="form-control"  required>
                    <option value="">Select one...</option>
                    <% for (var i = 0 ; i < checkout_list.length; i++) {%>
                            <option value="<%= checkout_list[i].wayBill %>"><%= checkout_list[i].wayBill%></option> 
                    <%}%>
                </select>
            </div>

            <div class="form-group">
            <span>Rider:</span>
                <select name="checkout" class="form-control"  required>
                    <option value="">Select one...</option>
                    <% for (var i = 0 ; i < messenger_list.length; i++) {%>
                        <option value="<%= messenger_list[i].userID %>"><%= messenger_list[i].firstName%> <%= messenger_list[i].lastName%></option> 
                    <%}%>
                </select>
            </div>
            
        </div>
      <div class="modal-footer">
      <input type="submit" class="btn btn-primary" value="Add Parcel">
        </form>
      </div>
    </div>
  </div>
</div>
