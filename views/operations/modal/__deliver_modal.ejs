<div class="modal fade" id="deliverModal" tabindex="-1" role="dialog" aria-labelledby="deliverModal" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Checkout Parcel</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form method="POST" id="dataget" action="/ops/deliverParcel">
            <div class="form-group">
                <span>WayBill:</span>
                <select name="wayBill" class="form-control"  required>
                    <option value="">Select one...</option>
                    <% for (var i = 0 ; i < deliver_list.length; i++) {%>
                            <option value="<%= deliver_list[i].wayBill %>"><%= deliver_list[i].wayBill%></option> 
                    <%}%>
                </select>
            </div>


            <div class="form-group">
            <span>Status:</span>
                <select name="status" class="form-control"  required>
                    <option value="">Select one...</option>
                    <option value="3">Delivered</option>
                    <option value="4">Returned</option>
                </select>
            </div>

            
        </div>
      <div class="modal-footer">
      <input type="submit" class="btn btn-primary" value="Add Parcel">
        </form>
      </div>
    </div>
  </div>
</div>